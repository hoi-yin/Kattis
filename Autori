import java.util.Scanner;
public class autori {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String[] a = sc.nextLine().split("-");
        
        for(int i = 0; i < a.length; i++) System.out.print(a[i].charAt(0));
        System.out.println();
    }
}
