import java.util.Scanner;
public class lastfactorialdigit {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int a ;
        a = sc.nextInt();
        
        for (int i = 1; i<=a ; i++){
            int b,c;
            c = 1;
            b = sc.nextInt();
            for(int j = 1 ; j<=b ; j++){
                c = c * j;
            }
            c = c%10;
            System.out.println(c);
        }
    }
}
