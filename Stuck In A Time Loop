import java.util.Scanner;
public class timeloop {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int a ;
        a = sc.nextInt();
        String b = "";
        
        for (int i = 1; i<=a ; i++){
            System.out.println( Integer.toString(i) + " Abracadabra");
        }
    }
}
